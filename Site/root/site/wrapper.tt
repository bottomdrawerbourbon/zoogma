            <span class="tourdate-info tourdate-city_state">[% stop.city %]</span>
          </li>
          [% END %]
          [% FOREACH stop IN shows %]
          <li class="tourdate">
            <span class="tourdate-info tourdate-date">[% stop.date %]</span>
            <span class="tourdate-info tourdate-venue">[% stop.venue %]</span>
            <span class="tourdate-info tourdate-city_state">[% stop.city %]</span>
          </li>
          [% END %]
          [% FOREACH stop IN shows %]
          <li class="tourdate">
            <span class="tourdate-info tourdate-date">[% stop.date %]</span>
            <span class="tourdate-info tourdate-venue">[% stop.venue %]</span>
            <span class="tourdate-info tourdate-city_state">[% stop.city %]</span>
          </li>
          [% END %]
          [% FOREACH stop IN shows %]
          <li class="tourdate">
            <span class="tourdate-info tourdate-date">[% stop.date %]</span>
            <span class="tourdate-info tourdate-venue">[% stop.venue %]</span>
            <span class="tourdate-info tourdate-city_state">[% stop.city %]</span>
