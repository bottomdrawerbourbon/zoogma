<script type="text/javascript" src="/static/js/lib/jquery.nicescroll.min.js"></script>
<script type="text/javascript">

$(document).ready(function(){

    // Setup scrollbars
    $('#tourdates-content').niceScroll({
        autohidemode: false,
        disableoutline: true,
        preservenativescrolling: true
    });

    // Setup image slider
    $('#features').coinslider({ 
        hoverPause: false,
        width: 700
    });

    $('#nav-band-link').click(function() {
        //$('#column_main').load('/band');
        $.getJSON('/band', function(result) {

            $('#column_main').fadeOut('fast', function() {
                // Animation complete.
            });

        });

        return false;
    });

});
</script>

</head>

<body>

<div id="container">
  <!-- begin header -->
  <div id="header">
    <div id="header-logo"></div>
    <div id="header-nav">

