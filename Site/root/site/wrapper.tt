<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html  xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>zoogma.net</title>

<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />

<!--[if lt IE 9]>
  <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
<![endif]-->

<!--[if IE 6]>

<link href="/css/ie6.css" rel="stylesheet" type="text/css" />

<![endif]-->

<link rel="stylesheet" type="text/css" href="/static/css/base.css" />
<link rel="stylesheet" type="text/css" href="/static/css/jplayer.blue.monday.css" />
<script type="text/javascript" src="/static/js/jquery-1.7.2.min.js"></script>
<script type="text/javascript" src="/static/js/jQuery.jPlayer.2.1.0/jquery.jplayer.min.js"></script>
<script type="text/javascript">

$(document).ready(function(){

    $("#jquery_jplayer_1").jPlayer({
        ready: function (event) {
            $(this).jPlayer("setMedia", {
                mp3: "/static/mp3/Daybroke.mp3" // Defines the mp3 url
            });
        },
        supplied: "mp3",
    });

});
</script>

</head>

<body>

<div id="container">
  <!-- begin header -->
  <div id="header">
    <div id="header-logo"></div>
    <div id="header-nav">

      <div id="header-nav-left"></div>

      <div id="header-nav-main">
        <ul>
          <li id="nav-home-link"></li>
          <li id="nav-band-link"></li>
          <li id="nav-shows-link"></li>
          <li id="nav-news-link"></li>
          <li id="nav-gallery-link"></li>
          <li id="nav-music-link"></li>
          <li id="nav-store-link"></li>
          <li id="nav-contact-link"></li>
        </ul>
      </div>
      <div id="header-nav-right"></div>
    </div>

    <!-- Begin jPlayer -->
    <div id="jquery_jplayer_1" class="jp-jplayer"></div>
    <div id="jp_container_1" class="jp-audio">
        <div class="jp-type-single">
            <div class="jp-gui jp-interface">
                <ul class="jp-controls">
                    <li><a href="javascript:;" class="jp-play" tabindex="1">play</a></li>
                    <li><a href="javascript:;" class="jp-pause" tabindex="1">pause</a></li>
                    <li><a href="javascript:;" class="jp-stop" tabindex="1">stop</a></li>
                    <li><a href="javascript:;" class="jp-mute" tabindex="1" title="mute">mute</a></li>
                    <li><a href="javascript:;" class="jp-unmute" tabindex="1" title="unmute">unmute</a></li>
                    <li><a href="javascript:;" class="jp-volume-max" tabindex="1" title="max volume">max volume</a></li>
                </ul>
                <div class="jp-progress">
                    <div class="jp-seek-bar">
                        <div class="jp-play-bar"></div>
                    </div>
                </div>

                <div class="jp-volume-bar">
                    <div class="jp-volume-bar-value"></div>
                </div>

                <div class="jp-current-time"></div>

                <div class="jp-duration"></div>

                <ul class="jp-toggles">
                    <li><a href="javascript:;" class="jp-repeat" tabindex="1" title="repeat">repeat</a></li>
                    <li><a href="javascript:;" class="jp-repeat-off" tabindex="1" title="repeat off">repeat off</a></li>
                </ul>
            </div>

            <div class="jp-title">
                <ul>
                    <li>Daybroke</li>
                </ul>
            </div>

            <div class="jp-no-solution">
                <span>Update Required</span>
                To play the media you will need to either update your browser to a recent version or update your <a href="http://get.adobe.com/flashplayer/" target="_blank">Flash plugin</a>.
            </div>
        </div>
    </div>
    <!-- End jPlayer -->

  </div>
  <!-- End Header -->

  <div id="main">

    <div id="tour-dates-col">
      [% FOREACH stop IN shows %]
        <div>[% stop.date %]</div>
        <div>[% stop.venue %]</div>
        <div>[% stop.city %]</div>
      [% END %]
    </div>
    <div id="column_main">

      [%~ content ~%]
    </div>
    <!-- Don't remove spacer div. Solve an issue about container height -->
    <div class="spacer"></div>
  </div>
  <div id="footer">This is my footer</div>
  </div>

</body>
</html>
